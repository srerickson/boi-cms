<li class="ui-state-default">
<span class="ui-icon ui-icon-arrow-4"></span>
<%= f.object.key %>

<a href="#" onclick="$('#schema_field_<%= f.object.id %>').toggle(200); return false;"><span class="ui-icon ui-icon-triangle-1-s"></span></a>

<div style="display:none" class="hide" id="schema_field_<%= f.object.id %>">

  <%# f.text_field :key, :style=>"width:200px"%></br>


  <%= f.label :short_name, :class=>"span-2"%>
  <%= f.text_field :short_name, :style=>"width:300px" %><br/>

  <%= f.label :description, :class=>"span-2" %>
  <%= f.text_area :description, :style=>"width:300px;height:75px;padding:1px;" %><br/>

  <%# f.label :funcs, :class=>"span-3" %>
  <%# f.text_area :funcs, :style=>"width:200px;height:100px;padding:1px;" %>


  <%= f.hidden_field :position, :class => "field_position" %>


  <!--
  <%# unless f.object.new_record? %>
    <span class="inline_attr destroy">
      Remove?
      <%# f.check_box :_destroy %>
    </span>
  <%# else %>
    <%# link_to_remove_fields "remove", f %>
  <%# end %>
  -->
</div>


</li>
